Class {
	#name : 'SpTabConstraints',
	#superclass : 'SpLayoutConstraints',
	#instVars : [
		'label'
	],
	#category : 'Spec2-Layout-Tab',
	#package : 'Spec2-Layout',
	#tag : 'Tab'
}

{ #category : 'accessing' }
SpTabConstraints >> label [

	^ label
]

{ #category : 'accessing' }
SpTabConstraints >> label: anObject [

	label := anObject
]
